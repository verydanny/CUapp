{
    "projectId": "app",
    "projectName": "My App",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "functions": [
        {
            "$id": "starter",
            "execute": [
                "users"
            ],
            "name": "starter",
            "enabled": true,
            "logging": true,
            "runtime": "bun-1.1",
            "scopes": [
                "users.read",
                "databases.read",
                "databases.write",
                "collections.read",
                "collections.write",
                "attributes.read",
                "attributes.write",
                "indexes.read",
                "indexes.write",
                "documents.read",
                "documents.write"
            ],
            "events": [],
            "schedule": "",
            "timeout": 15,
            "entrypoint": "src/main.ts",
            "commands": "bun install",
            "specification": "s-0.5vcpu-512mb",
            "path": "functions/starter"
        }
    ],
    "databases": [
        {
            "$id": "main",
            "name": "main",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "imessageMessages",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "main",
            "name": "imessageMessages",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "conversationId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "messageId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "participantDocId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "content",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 10000,
                    "default": null
                },
                {
                    "key": "timestamp",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "timestampDisplay",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "isEdited",
                    "type": "boolean",
                    "required": true,
                    "array": false,
                    "default": null
                },
                {
                    "key": "screenshotIndex",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": null
                },
                {
                    "key": "deliveryStatus",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "elements": [
                        "sent",
                        "delivered",
                        "read"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "messageType",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "elements": [
                        "text",
                        "image"
                    ],
                    "format": "enum",
                    "default": "text"
                },
                {
                    "key": "imageUrl",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 2048,
                    "default": null
                },
                {
                    "key": "imageFileId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "imageAltText",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 500,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "conversation_screenshot_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "conversationId",
                        "screenshotIndex"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                },
                {
                    "key": "conversation_timestamp_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "conversationId",
                        "timestamp"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                },
                {
                    "key": "conversation_message_unique_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "conversationId",
                        "messageId"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "profiles",
            "$permissions": [
                "read(\"users\")",
                "update(\"users\")"
            ],
            "databaseId": "main",
            "name": "profiles",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "username",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 40,
                    "default": null
                },
                {
                    "key": "bio",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 1500,
                    "default": null
                },
                {
                    "key": "profileImage",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "permissions",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "elements": [
                        "private"
                    ],
                    "format": "enum",
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "username_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "username"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "richTextPost",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "main",
            "name": "richTextPost",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "postId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "title",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "body",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 10000000,
                    "default": null
                },
                {
                    "key": "excerpt",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "coverImageFileId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "coverImageUrl",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 2048,
                    "default": null
                },
                {
                    "key": "coverImageAltText",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "version",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": null
                },
                {
                    "key": "estimatedReadTimeMinutes",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": null
                },
                {
                    "key": "contentSchemaVersion",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 50,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "post_id_unique_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "postId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "title_search_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "title"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "posts",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "main",
            "name": "posts",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "type",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "richTextPost",
                        "imessageConversation",
                        "imagePost"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "tags",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "status",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "published",
                        "draft",
                        "archived"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "accessLevel",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "public",
                        "private",
                        "followers",
                        "mutuals",
                        "unlisted",
                        "team"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "contentRefId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "likesCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": 0
                },
                {
                    "key": "commentsCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": 0
                }
            ],
            "indexes": [
                {
                    "key": "user_created_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": []
                },
                {
                    "key": "type_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "type"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "access_level_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "accessLevel"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "imessageParticipants",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "main",
            "name": "imessageParticipants",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "avatarFileId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "user_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "user_name_unique_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "userId",
                        "name"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "imessageConversation",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "main",
            "name": "imessageConversation",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "postId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "participantDocRefs",
                    "type": "string",
                    "required": true,
                    "array": true,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "rightSideParticipantDocRef",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "screenshotMessageIds",
                    "type": "string",
                    "required": true,
                    "array": true,
                    "size": 100000,
                    "default": null
                },
                {
                    "key": "totalScreenshots",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": 0,
                    "max": 99999999999999,
                    "default": null
                },
                {
                    "key": "conversationId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "post_id_unique_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "postId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        }
    ],
    "buckets": [
        {
            "$id": "images",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")"
            ],
            "fileSecurity": true,
            "name": "images",
            "enabled": true,
            "maximumFileSize": 30000000,
            "allowedFileExtensions": [],
            "compression": "gzip",
            "encryption": false,
            "antivirus": false
        }
    ]
}