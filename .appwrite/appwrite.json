{
    "projectId": "blog",
    "databases": [
        {
            "$id": "main",
            "name": "main",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "profiles",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")"
            ],
            "databaseId": "main",
            "name": "profiles",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "username",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 40,
                    "default": null
                },
                {
                    "key": "bio",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 1500,
                    "default": null
                },
                {
                    "key": "profileImage",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "permissions",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "elements": [
                        "private"
                    ],
                    "format": "enum",
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "username_index",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "username"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        }
    ]
}