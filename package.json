{
    "name": "cuapp",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "build:staging": "vite build --mode staging",
        "preview": "vite preview",
        "preview:staging": "vite preview --mode staging",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "storybook": "STORYBOOK=true storybook dev -p 6006",
        "build-storybook": "storybook build",
        "appwrite:types": "bun run -b appwrite/fetch-types",
        "bun:build": "bun build --packages=external --env=inline --minify-whitespace --minify-syntax --outfile build/bun/server ./build/index.js --compile"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.5.1",
        "@changesets/cli": "^2.29.5",
        "@eslym/sveltekit-adapter-bun": "^2.0.5",
        "@playwright/test": "^1.53.2",
        "@storybook/addon-docs": "^9.0.15",
        "@storybook/addon-svelte-csf": "^5.0.4",
        "@storybook/addon-vitest": "^9.0.15",
        "@storybook/sveltekit": "^9.0.15",
        "@sveltejs/adapter-node": "^5.2.12",
        "@sveltejs/kit": "^2.22.2",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/vite": "^4.1.11",
        "@testing-library/svelte": "^5.2.8",
        "@types/benchmark": "^2.1.5",
        "@types/bun": "^1.2.17",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.0.10",
        "@vitest/browser": "^3.2.4",
        "benchmark": "^2.1.4",
        "eslint": "^9.30.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-storybook": "^9.0.15",
        "eslint-plugin-svelte": "^3.10.1",
        "fetch-appwrite-types": "^2.2.2",
        "globals": "^16.3.0",
        "playwright": "^1.53.2",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.13",
        "storybook": "^9.0.15",
        "svelte-check": "^4.2.2",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "npm:rolldown-vite@latest",
        "vitest": "^3.2.4",
        "appwrite": "^18.1.1",
        "daisyui": "^5.0.43",
        "svelte": "^5.35.0"
    },
    "dependencies": {
        "node-appwrite": "^17.0.0"
    },
    "packageManager": "bun@1.2.17",
    "engines": {
        "node": "24.3.0",
        "bun": "1.2.17"
    },
    "trustedDependencies": [
        "@sveltejs/kit",
        "@tailwindcss/oxide",
        "esbuild",
        "svelte-preprocess"
    ],
    "overrides": {
        "storybook": "$storybook",
        "vite": "npm:rolldown-vite@latest"
    }
}
